{
  "name": "devscribe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "bun vite dev",
    "dev:bun": "bun --bun vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts",
    "db:generate": "drizzle-kit generate:mysql --config drizzle.config.ts",
    "db:migrate": "drizzle-kit push:mysql --config drizzle.config.ts",
    "db:clean": "drizzle-kit drop"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.138",
    "@skeletonlabs/skeleton": "^2.4.0",
    "@skeletonlabs/tw-plugin": "^0.2.3",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "mysql2": "^3.9.2",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "svelte": "^4.0.5",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.4.3",
    "sveltekit-search-params": "latest",
    "sveltekit-superforms": "^1.8.0",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unplugin-icons": "^0.17.3",
    "vite": "^5.0.10",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.17.0",
    "@auth/mongodb-adapter": "^2.0.2",
    "@auth/sveltekit": "^0.3.10",
    "@floating-ui/dom": "^1.6.1",
    "@planetscale/database": "^1.16.0",
    "@sveltestack/svelte-query": "^1.6.0",
    "ai": "^2.2.19",
    "axios": "^1.6.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "drizzle-orm": "^0.29.4",
    "events": "^3.3.0",
    "highlight.js": "^11.9.0",
    "marked": "^11.1.1",
    "mongoose": "^7.6.3",
    "openai": "^4.11.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  }
}